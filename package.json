{
  "name": "linkedin-email-autoresponder",
  "version": "0.1.0",
  "bin": {
    "linkedin-email-autoresponder": "bin/linkedin-email-autoresponder.js"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 tsc",
    "watch": "NODE_OPTIONS=--max_old_space_size=4096 tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.69.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.69.0-alpha.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "aws-cdk-lib": "^2.69.0",
    "aws-sdk": "^2.1338.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.294.0",
    "@aws-sdk/client-secrets-manager": "^3.294.0",
    "aws-lambda": "^1.0.7",
    "google-auth-library": "^8.7.0",
    "googleapis": "^113.0.0",
    "source-map-support": "^0.5.21"
  }
}
